  "scripts": {
    "format": "prettier --write --ignore-path .gitignore .",
    "checkformat": "prettier --check --ignore-path .gitignore .",
    "precommitformat": "npm run checkformat || (npm run format && false)",
    "lint": "eslint --ext .js,.jsx,.ts,.tsx --ignore-path .gitignore --fix .",
    "checklint": "eslint --ext .js,.jsx,.ts,.tsx --ignore-path .gitignore .",
    "precommitlint": "npm run checklint || (npm run lint && false)",
    "dev": "webpack serve --mode=development",
    "build": "webpack --mode=production",
    "deps": "npm-check-updates -u",
    "checkdeps": "npm-check-updates -e 2",
    "precommitdeps": "npm run checkdeps || (npm run deps && false)",
