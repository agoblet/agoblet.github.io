{
  "name": "axelgoblet",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "format": "prettier --write --ignore-path .gitignore .",
    "checkformat": "prettier --check --ignore-path .gitignore .",
    "precommitformat": "npm run checkformat || (npm run format && false)",
    "lint": "eslint --ext .js,.jsx,.ts,.tsx --ignore-path .gitignore --fix .",
    "checklint": "eslint --ext .js,.jsx,.ts,.tsx --ignore-path .gitignore .",
    "precommitlint": "npm run checklint || (npm run lint && false)",
    "build": "next build && next export && next-image-export-optimizer && rm -rf out/images",
    "deps": "npm-check-updates",
    "precommitdeps": "npm run deps || (npm i && false)",
    "deploy": "gh-pages -d out --dotfiles",
    "ci": "npm ci"
  },
  "dependencies": {
    "@emotion/react": "^11.10.4",
    "@emotion/styled": "^11.10.4",
    "@fontsource/moulpali": "^4.5.12",
    "@fontsource/roboto": "^4.5.8",
    "@mui/icons-material": "^5.10.9",
    "@mui/material": "^5.10.9",
    "next": "12.3.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "@mui/lab": "5.0.0-alpha.103",
    "react-ga4": "^1.4.1"
  },
  "devDependencies": {
    "@svgr/webpack": "^6.4.0",
    "@types/node": "18.8.5",
    "@types/react": "18.0.21",
    "@types/react-dom": "18.0.6",
    "eslint": "8.25.0",
    "eslint-config-next": "12.3.1",
    "next-image-export-optimizer": "^0.16.0",
    "typescript": "4.8.4",
    "@lhci/cli": "^0.9.0",
    "@typescript-eslint/eslint-plugin": "^5.40.0",
    "@typescript-eslint/parser": "^5.40.0",
    "eslint-config-google": "^0.14.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-react": "^7.31.10",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-unused-imports": "^2.0.0",
    "gh-pages": "^4.0.0",
    "npm-check-updates": "^16.3.11",
    "pre-commit": "^1.2.2",
    "prettier": "^2.7.1"
  },
  "pre-commit": [
    "precommitformat",
    "precommitlint",
    "precommitdeps",
    "ci",
    "build"
  ]
}
